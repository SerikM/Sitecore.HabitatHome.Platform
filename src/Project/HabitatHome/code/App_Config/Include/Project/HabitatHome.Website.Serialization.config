<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:unicorn="http://www.sitecore.net/xmlconfig/unicorn/">
	<sitecore unicorn:require="Enabled">
		<unicorn>
			<configurations>
				<configuration name="Project.HabitatHome.Website" description="Habitat Home content" dependencies="Foundation.*,Feature.*,Project.Global.Website" extends="Helix.Base">
					<targetDataStore physicalRootPath="$(sourceFolder)\Project\Bysoft\Content" useDataCache="false" singleInstance="true" />
					<predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
						<include name="Content" database="master" path="/sitecore/content/Main Tenant/Bysoft">
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Data/Commerce" />
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Data/Links/Store" />
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Presentation/Page Designs/Commerce" />
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Presentation/Partial Designs/Commerce" />
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Presentation/Styles/Custom/Store" />
							<exclude database="master" childrenOfPath="/sitecore/content/Main Tenant/Bysoft/Home/Product Catalog"/>
						</include>
						<include name="Templates.Branches" database="master" path="/sitecore/templates/Branches/Project/Main Tenant/Bysoft" />
						<include name="Layout.Renderings" database="master" path="/sitecore/layout/Renderings/Project/Main Tenant/Bysoft" />
						<include name="System.Settings" database="master" path="/sitecore/system/Settings/Project/Main Tenant/Bysoft" />
						<include name="Media" database="master" path="/sitecore/media library/Project/Main Tenant/Bysoft" />
						<include name="Forms" database="master" path="/sitecore/Forms/Main Tenant/Bysoft" />
					</predicate>
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>

